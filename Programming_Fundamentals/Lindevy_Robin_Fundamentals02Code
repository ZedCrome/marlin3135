int spaceBetweenLines = width / 10;
int yAxisOffset = 640;
int moduloCounter = 0;

void setup()
{
  size(640, 640);
  background(240);
  frameRate(5);
  ParabolicCurve ParabolicCurve1 = new ParabolicCurve(0, 0, 128);
  ParabolicCurve1.Print();
}
void draw() {

}
public class ParabolicCurve {

	int axis1;
	int axis2;
	int numberOfLines;

	public ParabolicCurve(int axis1, int axis2, int numberOfLines) {

		axis1 = axis1;
		axis2 = axis2;
		numberOfLines = numberOfLines;

		boolean curveRotation = false;
		for ( int i = 0; i < numberOfLines; i++) {
			strokeWeight(2);

			if (moduloCounter % 3 == 0) {
				stroke(0, 100);
				moduloCounter++;

			} else {
				stroke(170, 100);
				moduloCounter++;
			}
				axis1+= spaceBetweenLines;
				axis2 += spaceBetweenLines;

			line(0, axis1, axis2, 640);
			//println(axis1, axis2);
		}
	}
	public void Print() {
		println(axis1, axis2);
	}
}
